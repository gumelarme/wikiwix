{
	"name": "wikiwix",
	"version": "0.1",
	"description": "Open wikipedia page with offline kiwix.",
	"permissions": ["activeTab", "storage", "declarativeContent", "contextMenus"],
	"background": {
		"scripts" : ["background.js"],
		"persistent": false
	},
	"options_page": "options.html",
	"icons":{
		"16":"images/icon16.png",
		"32":"images/icon32.png",
		"42":"images/icon48.png",
		"128":"images/icon128.png"
	},
	"manifest_version": 2
}
